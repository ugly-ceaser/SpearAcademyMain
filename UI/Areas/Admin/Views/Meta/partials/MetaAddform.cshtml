@model DataTransfarLayer.MetaDataTransfer

  @Html.AntiForgeryToken()
    
    <div class="box-body">
        
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
      

        <input type="number" name="MetaId" value="model.MetaId" hidden />

        <div class="form-group">
            @Html.LabelFor(model => model.MetaName, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("MetaName",new SelectList( new [] {"Keyword", "Descriptions", "Author" }) , htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.MetaName, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.MetaContent, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.MetaContent, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.MetaContent, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>

<div>
   
</div>
